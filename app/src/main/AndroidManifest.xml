        <activity
            android:name=".activity.menu.MenuLoguin1Activity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.menu.MenuRegistroActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.menu.MenuInicioActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>